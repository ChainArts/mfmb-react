{
  "name": "mfmb-htl-hollabrunn",
  "productName": "MFMB",
  "title": "MFMB",
  "author": "Maximilian Roll",
  "description": "MFMB Application for HTL Hollabrunn",
  "build": {
    "appId": "com.MFMB.app",
    "win": {
      "icon": "./build/icons/ms-icon-310x310.png"
    },
    "files": [
      "public/**/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/ChainArts/mfmb-react"
  },
  "version": "0.5.0",
  "private": true,
  "main": "./public/electron.js",
  "homepage": "./",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.6.3",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "electron-is-dev": "^2.0.0",
    "framer-motion": "^3.10.6",
    "fs-extra": "^9.1.0",
    "get-video-duration": "^3.0.2",
    "klaw-sync": "^6.0.0",
    "mysql": "^2.18.1",
    "qrcode.react": "^1.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-icons": "^4.1.0",
    "react-particles-js": "^3.4.1",
    "react-player": "^2.9.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "sass": "^1.32.8",
    "simplebar-react": "^2.3.0",
    "swiper": "^6.4.15",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "electron-build": "electron-builder",
    "release": "npm react-build && electron-builder --publish=always",
    "build": "npm run react-build && npm run electron-build",
    "start": "concurrently \"cross-env BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react": "^17.0.1",
    "concurrently": "^6.0.0",
    "electron": "^12.0.2",
    "electron-builder": "^22.10.5",
    "wait-on": "^5.2.1"
  }
}
