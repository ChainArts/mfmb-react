{
  "name": "MFMB",
  "title": "MFMB",
  "author": "Maximilian Roll",
  "description": "MFMB Application for HTL Hollabrunn",
  "build": {
    "appId": "com.MFMB.app",
    "win": {
      "icon": "./build/icons/ms-icon-310x310.png"
    },
    "files": [
        "build/**/*",
        "node_modules/**/*"
    ]
  },
  "postinstall": "electron-builder install-app-deps",
  "version": "0.4.0",
  "private": true,
  "main": "./public/electron.js",
  "homepage": "./",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "electron-is-dev": "^1.2.0",
    "framer-motion": "^2.9.5",
    "mysql": "^2.18.1",
    "qrcode.react": "^1.0.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-icons": "^3.11.0",
    "react-particles-js": "^3.4.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.4",
    "simplebar-react": "^3.0.0-beta.9",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder",
    "release": "npm react-build && electron-builder --publish=always",
    "build": "npm run react-build && npm run electron-build",
    "start": "concurrently \"cross-env BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "electron": "^11.2.1",
    "electron-builder": "^22.9.1",
    "react-lorem-ipsum": "^1.4.9",
    "wait-on": "^5.2.1"
  }
}
